<Window x:Class="Assignment_2.Window_Component.Views.ViewMenuViews.ViewAllMembersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment_2.Window_Component.Views.ViewMenuViews"
        mc:Ignorable="d"
        Title="ViewAllMembersWindow" Height="450" Width="800">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBlock Text="All members"/>

        <DataGrid Grid.Row="1" ItemsSource="{Binding ViewData}">
        </DataGrid>
    </Grid>
</Window>
